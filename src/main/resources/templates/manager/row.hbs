<tr id="managerItem{{cpf}}">
    <td style="width: 100px; text-align: left;">{{name}}</td>
    <td style="width: 200px; text-align: left;">{{cpf}}</td>
    <td style="width: 300px; text-align: left;">{{phone}}</td>
    <td style="width: 300px; text-align: left;">{{email}}</td>
    <td style="width: 50px">

        <img src="https://img.icons8.com/?size=18&id=71201&format=png&color=000000"
             alt="Editar item"
             style="cursor: pointer"
             hx-target="#managerForm"
             hx-replace="innerHTML"
             hx-get="manager/edit?id={{id}}">

        <img src="https://img.icons8.com/?size=18&id=67884&format=png&color=000000"
             alt="Apagar item"
             style="cursor: pointer"
             hx-target="#managerItemsTable"
             hx-select="#managerItemsTable"
             hx-replace="innerHTML"
             hx-get="manager/delete?id={{id}}"
        hx-include="[name='page']">
    </td>
</tr>