<tr id="veiculoItem{{id}}">
    <td>{{id}}</td>
    <td>{{marca}}</td>
    <td>{{modelo}}</td>
    <td>{{placa}}</td>
    <td>{{cor}}</td>
    <td>{{anoFabricacao}}</td>
    <td>{{custoDiaria}}</td>
    <td>{{descricaoPromocional}}</td>
    <td>{{tipoCombustivel}}</td>
    <td style="text-align: center;">
        <img src="https://img.icons8.com/?size=18&id=71201&format=png&color=000000"
             alt="Editar item"
             style="cursor: pointer"
             hx-target="#formAndTable"
             hx-replace="innerHTML"
             hx-get="veiculos/edit?id={{id}}">
    </td>
    <td style="text-align: center;">
        <img src="https://img.icons8.com/?size=18&id=67884&format=png&color=000000"
             alt="Apagar item"
             style="cursor: pointer"
             hx-target="#formAndTable"
             hx-select="#formAndTable"
             hx-replace="innerHTML"
             hx-get="veiculos/delete?id={{id}}"
        hx-include="[name='pageClient']">
    </td>
</tr>