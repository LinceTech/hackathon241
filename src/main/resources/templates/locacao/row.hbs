<tr id="locacaoItem{{id}}">
    <td style="width: 40px; text-align: right;">{{dataInicio}}</td>
    <td style="width: 200px; text-align: left;">{{gerenteCpf}}</td>
    <td style="width: 300px; text-align: left;">{{clienteCpf}}</td>
    <td style="width: 300px; text-align: left;">{{placaVeiculo}}</td>
    <td style="width: 50px">
        <img src="https://img.icons8.com/?size=18&id=71201&format=png&color=000000"
             alt="Editar item"
             style="cursor: pointer"
             hx-target="#locacaoForm"
             hx-replace="innerHTML"
             hx-get="locacao/edit?id={{id}}">
    </td>
</tr>