<tr id="veiculoItem{{id}}">
    <td style="width: 40px; text-align: right;">{{id}}</td>
    <td style="width: 10px;">&nbsp;</td>
    <td style="width: 200px; text-align: left;">{{marca}}</td>
    <td style="width: 200px; text-align: left;">{{modelo}}</td>
    <td style="width:  75px; text-align: left;">{{placa}}</td>
    <td style="width: 200px; text-align: left;">{{cor}}</td>
    <td style="width: 50px">
        <img src="https://img.icons8.com/?size=18&id=71201&format=png&color=000000"
             alt="Editar item"
             style="cursor: pointer"
             hx-target="#veiculoForm"
             hx-replace="innerHTML"
             hx-get="veiculo/edit?id={{id}}">

        <img src="https://img.icons8.com/?size=18&id=67884&format=png&color=000000"
             alt="Apagar item"
             style="cursor: pointer"
             hx-target="#formAndTable"
             hx-select="#formAndTable"
             hx-replace="innerHTML"
             hx-get="veiculo/delete?id={{id}}"
             hx-include="[name='page']">
    </td>
</tr>