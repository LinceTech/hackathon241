<tr id="gerenteItem{{nr_cpf}}">
    <td class="formataCpf" style="text-align: left;">{{nr_cpf_tel}}</td>
    <td style="text-align: left;">{{nm_gerente}}</td>
    <td style="text-align: left;">{{ds_email}}</td>
    <td class="formataData" style="text-align: left;">{{dt_contratacao_tel}}</td>
    <td style="text-align: left;">{{dt_nascimento_tel}}</td>
    <td class="formataTel" style="text-align: left;">{{nr_telefone_tel}}</td>
    <td style="text-align: left;">{{nm_cidade}}</td>
    <td style="text-align: left;">{{nm_estado}}</td>
    <td class="formataCom" style="text-align: left;">{{pc_comissao}}</td>
    <td>
        <img src="https://img.icons8.com/?size=18&id=71201&format=png&color=000000"
             alt="Editar item"
             style="cursor: pointer"
             hx-target="#gerenteForm"
             hx-replace="innerHTML"
             hx-get="veiculo/edit?nr_cpf={{nr_cpf}}">

        <img src="https://img.icons8.com/?size=18&id=67884&format=png&color=000000"
             alt="Apagar item"
             style="cursor: pointer"
             hx-target="#formAndTable"
             hx-select="#formAndTable"
             hx-replace="innerHTML"
             hx-get="veiculo/delete?nr_cpf={{nr_cpf}}"
             hx-include="[name='page']">
    </td>
</tr>