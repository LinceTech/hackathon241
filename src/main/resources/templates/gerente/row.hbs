<tr id="gerenteItem{{bar}}">
    <td style="width: 250px; text-align:  left;">{{nm_gerente}}</td>
    <td style="width: 85px;  text-align: right;">{{nr_cpf}}</td>
    <td style="width: 300px; text-align: right;">{{ds_email}}</td>
    <td style="width: 50px">
        <img src="https://img.icons8.com/?size=18&id=71201&format=png&color=000000"
             alt="Editar item"
             style="cursor: pointer"
             hx-target="#gerenteForm"
             hx-replace="innerHTML"
             hx-get="gerente/edit?nr_cpf={{nr_cpf}}">

        <img src="https://img.icons8.com/?size=18&id=67884&format=png&color=000000"
             alt="Apagar item"
             style="cursor: pointer"
             hx-target="#formAndTable"
             hx-select="#formAndTable"
             hx-replace="innerHTML"
             hx-get="gerente/delete?nr_cpf={{nr_cpf}}"
             hx-include="[name='page']">
    </td>
</tr>