<tr id="gerenteItem{{id}}">
    <td style="width: 40px; text-align: right;">{{id}}</td>
    <td style="width: 200px; text-align: left;">{{nome}}</td>
    <td style="width: 200px; text-align: left;">{{email}}</td>
    <td style="width: 200px; text-align: left;">{{cidade}}</td>
    <td style="width: 200px; text-align: left;">{{estado}}</td>
    <td style="width: 200px; text-align: left;">{{comissao}}</td>
    <td style="width: 200px; text-align: left;">{{dtContrataFormat}}</td>
    <td style="width: 50px">
        <img src="https://img.icons8.com/?size=18&id=71201&format=png&color=000000"
             alt="Editar item"
             style="cursor: pointer"
             hx-target="#gerenteForm"
             hx-replace="innerHTML"
             hx-get="gerente/edit?id={{id}}">

        <img src="https://img.icons8.com/?size=18&id=67884&format=png&color=000000"
             alt="Apagar item"
             style="cursor: pointer"
             hx-target="#formAndTable"
             hx-select="#formAndTable"
             hx-replace="innerHTML"
             hx-get="gerente/delete?id={{id}}"
             hx-include="[name='page']">
    </td>
</tr>